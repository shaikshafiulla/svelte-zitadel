{
	"name": "ext-proj",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"setup:zitadel": "tsx scripts/setup-zitadel.ts",
		"cleanup:users": "tsx scripts/cleanup-users.ts",
		"dev:api": "tsx src/server/index.ts",
		"dev:all": "npx concurrently \"npm run dev\" \"npm run dev:api\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.2.0",
		"concurrently": "^9.2.1",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@types/express": "^5.0.6",
		"awilix": "^12.0.5",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"lucide-svelte": "^0.563.0",
		"openid-client": "^6.8.1",
		"zod": "^4.3.6"
	}
}
